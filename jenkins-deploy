	step([$class: 'WsCleanup'])
	stage('代码下载') {
		checkout([$class: 'GitSCM', branches: [[name: '*/sccc-4.0.4']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'chenbao', url: 'http://172.16.68.11:17888/pass/profile-parent.git']]])
	}
    stage('deploy') {
		sh label: '', script: 'sh /home/shangc/apache-maven-3.5.4/bin/mvn clean deploy -Dpmd.skip=true -Dmaven.test.skip'
    }
}